Uref	10;
Iu		0.01; /* turbulence intensity */
L			10.0; /* turbulence length scale */

Cmu		0.09;
kappa	0.41;



kInlet #eval " 3* pow( $Uref * $Iu , 2 )  / 2 ";

epsilonInlet #eval " pow( $Cmu , 0.75 )  * pow( $kInlet , 1.5 ) / $L ";

omegaInlet #eval " pow( $kInlet , 0.5 )  / pow( $Cmu , 0.25 ) / $L ";




